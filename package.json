{
  "name": "nexss-vscode-nexss",
  "publisher": "nexss",
  "license": "MIT",
  "homepage": "https://nexss.com",
  "displayName": "Nexss.com",
  "description": "Nexss Programmer extension for VSCode",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.42.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "nexss",
        "aliases": [
          "Nexss",
          "nexss"
        ],
        "extensions": [
          ".nexss"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nexss",
        "scopeName": "source.nexss",
        "path": "./syntaxes/nexss.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mapoart/vscode_nexss.git"
  },
  "bugs": {
    "url": "https://github.com/mapoart/vscode_nexss/issues"
  },
  "galleryBanner": {
    "color": "#1058124",
    "theme": "light"
  },
  "icon": "assets/a_nexss.png"
}
